{
	"extends": ["airbnb", "prettier"],
	"env": {
		"browser": true,
		"node": true,
		"jest": true
	},
	"parserOptions": {
		"ecmaVersion": 11,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"impliedStrict": true,
			"classes": true
		}
	},
	"plugins": ["prettier"],
	"rules": {
		"semi": "off"
	}
}
